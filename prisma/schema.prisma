generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Slider {
  id        String   @id @default(cuid())
  arSlider  String
  enSlider  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model StoreImage {
  id                   String   @id @default(cuid())
  storeLogo            String
  categoryDefaultImage String
  productDefaultImage  String
  storeIcon            String
  vatLogo              String
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
}
