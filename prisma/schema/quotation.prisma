model Quotation {
  id            String   @id @default(cuid())
  quoteNumber   String
  userId        String
  model         String
  enDescription String
  arDescription String
  price         Decimal  @default(0.0)
  quantity      Int      @default(0)
  image         String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@map("quotations")
}
